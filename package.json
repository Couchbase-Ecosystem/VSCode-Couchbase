      {
        "command": "vscode-couchbase.showOutputConsole",
        "title": "Show Output Console",
        "category": "Couchbase"
      },
      {
        "command": "vscode-couchbase.runQuery",
        "title": "Run Query",
        "category": "Couchbase",
        "icon": "$(play)",
        "tooltip": "Run Query"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "vscode-couchbase.runQuery",
          "category": "Couchbase",
          "when": "editorLangId == sqlpp || resourceFilename =~ /.sqlpp$/ ",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "vscode-couchbase.runQuery",
          "when": "editorLangId == sqlpp || resourceFilename =~ /.sqlpp$/ ",
          "group": "navigation@1"
        }
      ],
