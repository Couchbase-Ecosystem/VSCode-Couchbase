    "keybindings": [
      {
        "command": "vscode-couchbase.runQuery",
        "key": "ctrl+shift+e",
        "mac": "cmd+shift+e",
        "when": "(editorLangId == sqlpp || resourceFilename =~ /.sqlpp$/) && !isKVCluster"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "vscode-couchbase.runQuery",
          "category": "Couchbase",
          "when": "(editorLangId == sqlpp || resourceFilename =~ /.sqlpp$/)  && !isKVCluster",
          "group": "navigation@1"
        },
        {
          "title": "Query Context",
          "command": "vscode-couchbase.queryContext",
          "category": "Couchbase",
          "group": "navigation@2",
          "when": "(editorLangId == sqlpp || resourceFilename =~ /.sqlpp$/)  && !isKVCluster"
        },
        {
          "title": "Show Favorite Queries",
          "command": "vscode-couchbase.showFavoriteQueries",
          "category": "Couchbase",
          "group": "navigation@3",
          "when": "(editorLangId == sqlpp || resourceFilename =~ /.sqlpp$/)  && !isKVCluster"
