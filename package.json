    "languages": [
      {
        "id": "SQL++",
        "extensions": [
          ".n1qlnb",
          ".n1ql",
          ".sqlpp"
        ],
        "aliases": [
          "SQL++"
        ],
        "configuration": "./language/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "SQL++",
        "scopeName": "source.n1ql",
        "path": "./language/n1ql.tmLanguage"
      }
    ],
    "customEditors": [
      {
        "viewType": "vscode-couchbase.couchbaseDocument",
        "displayName": "Couchbase JSON Document Editor",
