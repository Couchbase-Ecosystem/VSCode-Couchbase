  "scripts": {
    "build": "webpack",
    "format": "prettier 'src/**/*.ts' --write",
    "lint": "eslint src --ext ts --fix",
    "package": "vsce package",
    "pretest": "npm run test-compile && npm run lint",
    "rebuild": "npm run rebuild:openssl && npm run rebuild:cmake && npm run rebuild:binding",
    "rebuild:openssl": "sed -i '' 's/COUCHBASE_CXX_CLIENT_POST_LINKED_OPENSSL ON/COUCHBASE_CXX_CLIENT_POST_LINKED_OPENSSL OFF/' node_modules/couchbase/CMakeLists.txt",
    "rebuild:cmake": "cmake-js rebuild -d node_modules/couchbase -r electron -v 19.1.8",
    "rebuild:binding": "mkdir -p ./build && cp node_modules/couchbase/build/Release/couchbase_impl.node ./build",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "webpack --devtool source-map",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^8.2.3",
